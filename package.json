{
  "name": "resume",
  "version": "1.0.0",
  "private": true,
  "author": "andrechristikan@gmail.com",
  "description": "Personal project",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && node ./dist/src/app.js",
    "build": "rimraf ./dist/src && babel ./src/app.js -o ./dist/src/ --config-file ./.babelrc --copy-files",
    "dev": "nodemon --exec babel-node ./src/app.mjs",
    "pre": "npm install && npm install --only=dev",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx,cjs,mjs}' ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
